{
    "Version": "2012-10-17",
    "Statement": [
      {
        "Effect": "Allow",
        "Action": [
          "ssm:Describe*",
          "ssm:List*",
          "ssm:Get*",
          "s3:List*",
          "s3:Get*",
          "s3:Describe*",
          "cloudwatch:Get*",
          "cloudwatch:List*"
        ],
        "Resource": [
          "*"
        ]
      },
      {
        "Effect": "Allow",
        "Action": [
          "s3:PutObject",
          "s3:ListBucket",
          "s3:GetObject",
          "s3:GetBucketLocation",
          "s3:DeleteObject"
        ],
        "Resource": [
          "arn:aws:s3:::optum-analyst-medicare-backend-us-west-2-dev/*"
        ]
      }
    ]
  }